{
  "name": "@goa/router",
  "version": "0.0.0-pre",
  "description": "The Router For Creating Middleware For Goa Apps.",
  "main": "compile/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "compile": "depack src/bin/router -o compile/bin/router.js -a -c -S --externs types/externs.js",
    "lib": "depack src/depack.js -o compile/router.js -a -c --source_map_include_content",
    "test-compile": "ALAMODE_ENV=test-compile yarn test",
    "lint": "eslint .",
    "doc": "NODE_DEBUG=doc doc -o README.md -n _router",
    "wiki": "NODE_DEBUG=doc doc documentary-wiki -W wiki -n _router",
    "d": "yarn-s template d1 externs",
    "d1": "typal compile/index.js -u -t types/index.xml",
    "template": "typal compile/template.js -T compile/index.js -t types/api.xml",
    "externs": "typal types/externs.js"
  },
  "files": [
    "src",
    "compile",
    "types/externs.js"
  ],
  "externs": "types/externs.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/idiocc/goa-router.git"
  },
  "keywords": [
    "router",
    "goa",
    "server",
    "middleware"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/idiocc/goa-router/issues"
  },
  "homepage": "https://github.com/idiocc/goa-router#readme",
  "devDependencies": {
    "@contexts/http": "^1.5.1",
    "@externs/goa": "^0.1.1",
    "@goa/compose": "^1.0.0",
    "@goa/http-errors": "^1.0.0",
    "@goa/koa": "^1.1.5",
    "@idio/debug": "^1.2.3",
    "@typedefs/goa": "^0.0.6",
    "alamode": "^3.3.0",
    "depack": "^1.0.1",
    "documentary": "^1.34.2",
    "erte": "^1.2.0",
    "eslint-config-artdeco": "1.0.1",
    "rqt": "^4.0.0",
    "yarn-s": "1.2.1",
    "zoroaster": "^4.1.2"
  },
  "dependencies": {
    "urijs": "^1.19.2"
  }
}
